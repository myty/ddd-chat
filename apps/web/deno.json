{
  "name": "@myty/fresh-workspace-web",
  "version": "0.0.0",
  "exports": {},
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.2",
    "@headlessui/react": "npm:@headlessui/react@^2.2.0",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.0.0",
    "@tanstack/react-router": "npm:@tanstack/react-router@^1.99.0",
    "@tanstack/router-devtools": "npm:@tanstack/router-devtools@^1.99.0",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.99.3",
    "@types/react": "npm:@types/react@^19.0.2",
    "@types/react-dom": "npm:@types/react-dom@^19.0.2",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "tailwindcss": "npm:tailwindcss@^4.0.0",
    "vite": "npm:vite@^6.0.5"
  }
}
